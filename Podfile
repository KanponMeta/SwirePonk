# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'



def swireponk_app
  pod 'Masonry'
  pod 'AFNetworking'
  pod 'MJRefresh'
  pod 'TMCache'
  pod 'YYModel'
end

def swireponk_tools
  pod 'FLEX', :configurations => ['DEBUG']
#  pod 'DoraemonKit', :configurations => ['DEBUG']
end

target 'SwirePonk' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwirePonk
  swireponk_app
  swireponk_tools
end

  
